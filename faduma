const mouseOver = document.querySelector(".logo-heading")

mouseOver.addEventListener("mouseover", (event) => {
  event.target.style.transform="scale(1.5)"
  console.log(event.target)
})

const navigation = document.querySelectorAll(".nav-link")
navigation.forEach(function(ite) {ite.addEventListener("click", (event)=>{event.target.style.backgroundColor="blue"})})

const header = document.querySelectorAll("h2")
header.forEach(function(items){items.addEventListener("dblclick", (event)=>{event.target.style.transform="scale(1.5)";
                                                                           event.target.style.color="green";
                                                                           console.log(event.target)})})

const paragraph=document.querySelectorAll("p")
paragraph.forEach(function(items){items.addEventListener("dragover", (event)=>{event.target.style.color="orange";
                                                                              console.log(event.target)})})

window.addEventListener("load", (event)=>{console.log("page is full loaded")})

const button=document.querySelectorAll(".btn")

button.forEach(function(bttns){bttns.addEventListener("wheel", (event)=>{event.target.style.backgroundColor="yellow";console.log(event.target)})})

let copyright=document.querySelector(".copyright")
copyright.addEventListener("select", (event)=>{event.target.style.backgroundColor="green"})

